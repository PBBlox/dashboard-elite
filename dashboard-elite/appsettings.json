{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Dimensions": {

    "FullScreenBorder": 2,
    "Minimized": false,

    "yyZoom": 100,
    "yyHeight": 665,
    "yyWidth": 2210,
    "yyTop": 100,
    "yyLeft": 100,
    "yyFullScreen": false,

    "xxZoom": 175,
    "xxHeight": 1104,
    "xxWidth": 3844,
    "xxTop": 1454,
    "xxLeft": 531,
    "xxFullScreen": true,

    "zzZoom": 100,
    "zzHeight": 665,
    "zzWidth": 2210,
    "zzTop": 100,
    "zzLeft": 100,
    "zzFullScreen": false,

    "Zoom": 100,
    "Height": 568,
    "Width": 1800,
    "Top": 100,
    "Left": 100,
    "FullScreen": false

  },
  "Windows" : 1, 
  "ExternalPort": 0,
  "ClickSound": "",
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "dashboard-elite"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "fileSizeLimitBytes": "5000000",
          "rollOnFileSizeLimit": "True",
          "retainedFileCountLimit": "60",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "MySink",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}
