@using dashboard_elite.Helpers
@using EliteJournalReader.Events
<MudNavMenu class="large-font">
    <MudNavGroup Title="Navigation" Expanded="@(NavSection == "navigation")">
        <MudNavLink Href="@("navigation/" + PageHelper.Page.Navigation)" Match="NavLinkMatch.Prefix" >Navigation</MudNavLink>
        <MudNavLink Href="@("navigation/" + PageHelper.Page.Route)" Match="NavLinkMatch.Prefix" >Route</MudNavLink>
        <MudNavLink Href="@("navigation/" + PageHelper.Page.SystemMap)" Match="NavLinkMatch.Prefix" >System Map</MudNavLink>
        <MudNavLink Href="@("navigation/" + PageHelper.Page.OdysseySettlements)" Match="NavLinkMatch.Prefix" >OD. Settlements</MudNavLink>
        <MudNavLink Href="@("navigation/" + PageHelper.Page.ColoniaBridge)" Match="NavLinkMatch.Prefix" >Colonia Bridge</MudNavLink>
    </MudNavGroup>
    <MudNavGroup Title="Information" Expanded="@(NavSection == "information")">
        <MudNavLink Href="@("information/" + PageHelper.Page.Commander)" Match="NavLinkMatch.Prefix" >Commander</MudNavLink>
        <MudNavLink Href="@("information/" + PageHelper.Page.Missions)" Match="NavLinkMatch.Prefix" >Missions</MudNavLink>
        <MudNavLink Href="@("information/" + PageHelper.Page.Galnet)" Match="NavLinkMatch.Prefix" >Galnet</MudNavLink>
        <MudNavLink Href="@("information/" + PageHelper.Page.ChatLog)" Match="NavLinkMatch.Prefix" >Chat Log</MudNavLink>
        <MudNavLink Href="@("information/" + PageHelper.Page.HWInfo)" Match="NavLinkMatch.Prefix" >HWInfo</MudNavLink>
    </MudNavGroup>
    <MudNavGroup Title="Ship" Expanded="@(NavSection == "ship")">
        <MudNavLink Href="@("ship/" + PageHelper.Page.Target)" Match="NavLinkMatch.Prefix" >Target</MudNavLink>
        <MudNavLink Href="@("ship/" + PageHelper.Page.CurrentShip)" Match="NavLinkMatch.Prefix" >Current Ship</MudNavLink>
        <MudNavLink Href="@("ship/" + PageHelper.Page.StoredShips)" Match="NavLinkMatch.Prefix" >Stored Ships</MudNavLink>
        <MudNavLink Href="@("ship/" + PageHelper.Page.StoredModules)" Match="NavLinkMatch.Prefix" >Stored Modules</MudNavLink>
        <MudNavLink Href="@("ship/" + PageHelper.Page.Materials)" Match="NavLinkMatch.Prefix" >Materials</MudNavLink>
        <MudNavLink Href="@("ship/" + PageHelper.Page.Cargo)" Match="NavLinkMatch.Prefix" >Cargo</MudNavLink>
        <MudNavLink Href="@("ship/" + PageHelper.Page.EdEngineer)" Match="NavLinkMatch.Prefix"  Disabled="@string.IsNullOrEmpty(Engineer.CommanderName)">ED Engineer</MudNavLink>
    </MudNavGroup>
    <MudNavGroup Title="Suit" Expanded="@(NavSection == "suit")">
        <MudNavLink Href="@("suit/" + PageHelper.Page.ShipLocker)" Match="NavLinkMatch.Prefix" >Ship Locker</MudNavLink>
        <MudNavLink Href="@("suit/" + PageHelper.Page.Backpack)" Match="NavLinkMatch.Prefix" >Backpack</MudNavLink>
    </MudNavGroup>
    <MudNavGroup Title="Locations" Expanded="@(NavSection == "locations")">
        <MudNavLink Href="@("locations/" + PageHelper.Page.Poi)" Match="NavLinkMatch.Prefix" >Points of Interest</MudNavLink>
        <MudNavLink Href="@("locations/" + PageHelper.Page.Engineers)" Match="NavLinkMatch.Prefix" >Engineers</MudNavLink>
        <MudNavLink Href="@("locations/" + PageHelper.Page.Powers)" Match="NavLinkMatch.Prefix" >Powers</MudNavLink>
        <MudNavLink Href="@("locations/" + PageHelper.Page.Mining)" Match="NavLinkMatch.Prefix" >Mining</MudNavLink>
        @*<MudNavLink Href="@("locations/" + PageHelper.Page.GalaxyMap)" Match="NavLinkMatch.Prefix" >Galaxy Map</MudNavLink> *@
    </MudNavGroup>
</MudNavMenu>